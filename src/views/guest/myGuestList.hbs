            <tr>
                <th class="mdl-data-table__cell--non-numeric">Nombre</th>
                <th class="mdl-data-table__cell--non-numeric">Correo</th>
                <th class="mdl-data-table__cell--non-numeric">Teléfono</th>
                <th class="mdl-data-table__cell--non-numeric">Organización</th>
                <th class="mdl-data-table__cell--non-numeric">Fecha y hora</th>
            </tr>
        </thead>
        <tbody>
            {{#each guests }}
            {{> myGuestRow }}
            {{/each}}
        </tbody>
    </table>
</div>